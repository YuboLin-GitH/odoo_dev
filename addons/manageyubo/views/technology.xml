<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="vista_manageyubo_technology_tree">
      <field name="name">vista_manageyubo_technology_tree</field>
      <field name="model">manageyubo.technology</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="description"/>
          <field name="photo"/>
        </tree>
      </field>
    </record>



    <record model="ir.ui.view" id="vista_manageyubo_technology_form">
      <field name="name">vista_manageyubo_technology_form</field>
      <field name="model">manageyubo.technology</field>
      <field name="arch" type="xml">
        <form string="formulario_task">
          <sheet>
            <group name="group_top">
                <field name="name"/>
                <field name="description"/>
                <field name="photo"/>
                <field name="task_id"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    


<record model="ir.actions.act_window" id="accion_manageyubo_technology_form">
      <field name="name">Listado de Tecnología</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">manageyubo.technology</field>
      <field name="view_mode">tree,form</field>
      <field name="help" type="html">
        <p class="oe_view_nocontent_create">
          Tecnología
        </p>
        <p> Click <strong> 'Crear' </strong> para añadir nuevos elementos
         </p>
      </field>
</record>



    <!-- actions -->

    <menuitem name="Technology" id="menu_manageyubo_technology" parent="menu_manageyubo_management"
              action="accion_manageyubo_technology_form"/>
   

  </data>
</odoo>
